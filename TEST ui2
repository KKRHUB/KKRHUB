-- Scripts:

local function NGNK_fake_script() -- KKRHUB.LocalScript 
	local script = Instance.new('LocalScript', KKRHUB)

	print("KKR HUB")
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = ":)"; -- The text
		Icon = "rbxassetid://13440848818";
		Duration = "3"; -- How much will the warning be visible
	})
	-------------------------------------------
	script.Parent.Frame2.TextButton.MouseButton1Click:Connect(function()
		script.Parent.Frame.Visible = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "Open"; -- The text
			Icon = "rbxassetid://13867768098";
			Duration = "3"; -- How much will the warning be visible
		})
	end)
	
	script.Parent.Frame.F2.CL.MouseButton1Click:Connect(function()
		script.Parent.Frame.Visible = false
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "Close"; -- The text
			Icon = "rbxassetid://13867768098";
			Duration = "3"; -- How much will the warning be visible
		})
	end)
end
coroutine.wrap(NGNK_fake_script)()
