-- Scripts:

local function LDQJRK_fake_script() -- KKRHUB.LocalScript 
	local script = Instance.new('LocalScript', KKRHUB)

	local F = script.Parent.Frame
	local BM1 = script.Parent.Frame.BM1
	local BM2 = script.Parent.Frame.BM2
	local OP1 = script.Parent.Frame.OP1
	local OP2 = script.Parent.Frame.OP2
	local CL = script.Parent.Frame.CL
	
	OP1.MouseButton1Click:Connect(function()
		BM1.Visible = true
		BM2.Visible = false
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "เปิด Ui เรียบร้อย"; -- The text
			Duration = "3"; -- How much will the warning be visible
		})
	end)
	
	OP2.MouseButton1Click:Connect(function()
		BM1.Visible = false
		BM2.Visible = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "เปิด Ui เรียบร้อย"; -- The text
			Icon = "rbxassetid://13867768098";
			Duration = "3"; -- How much will the warning be visible
		})
	end)
	-----------------------------------------------------------
	
	--------------------------------------------------------
	
	local BGUI = script.Parent["BGUI M"].ImageButton
	local BGUI2 = script.Parent["BGUI M"].ImageButton2
	
	BGUI.MouseButton1Click:Connect(function()
		print("เริ่มใช้งาน KKR HUB")
		F.Visible = true
		BGUI.Visible = false
		BGUI2.Visible = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "เปิด Ui เรียบร้อย"; -- The text
			Duration = "3"; -- How much will the warning be visible
		})
	end)
	---------------------------------------------------------
	BGUI2.MouseButton1Click:Connect(function()
		F.Visible = false
		BGUI.Visible = true
		BGUI2.Visible = false
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "ปิด Ui เรียบร้อย"; -- The text
			Duration = "3"; -- How much will the warning be visible
		})
	end)
	---------------------------------------------------------
	CL.MouseButton1Click:Connect(function()
		BM1.Visible = false
		BM2.Visible = false
		F.Visible = false
		BGUI2.Visible = false
		BGUI.Visible = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "ปิด UI ทั้งหมด"; -- The text
			Duration = "3"; -- How much will the warning be visible
		})
	end)
	-------------------------------------------------------------------
	-- GUI {}
	-------------------------------------------------------------------
	
	local SPEED = script.Parent.Frame.BM2.speed
	local SPEED2 = script.Parent.Frame.BM2.speed2
	
	SPEED.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
		SPEED.Visible = false
		SPEED2.Visible = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "วิ่งเร็ว สำเร็จ"; -- The text
			Duration = "3"; -- How much will the warning be visible
		})
	end)
	
	SPEED2.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
		SPEED.Visible = true
		SPEED2.Visible = false
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "ปิดโหมด วิ่งเร็ว"; -- The text
			Duration = "3"; -- How much will the warning be visible
		})
	end)
end
