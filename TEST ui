-- Scripts:

local function ZYZDNT_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	print("KKR")
	wait(1)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "1/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	wait(1)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "2/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	wait(1)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "3/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	wait(1)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "4/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	wait(2)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "5/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "finish"; -- The text
		Icon = "rbxassetid://13867768098";
		Duration = "3"; -- How much will the warning be visible
	})
	script.Parent.Visible = false
	script.Parent.Parent.FramePC.Visible = true
end
coroutine.wrap(ZYZDNT_fake_script)()
local function ICSB_fake_script() -- FramePC.LocalScript 
	local script = Instance.new('LocalScript', FramePC)

	script.Parent.Parent.FramePC.Draggable = true
	script.Parent.Parent.FramePC.Active = true
	
end
coroutine.wrap(ICSB_fake_script)()
