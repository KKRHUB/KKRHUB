-- Scripts:

local function KSSKJ_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	wait(1)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "1/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	wait(1)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "2/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	wait(1)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "3/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	wait(1)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "4/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	wait(2)
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "5/5"; -- The text
		Icon = "rbxassetid://13867779321";
		Duration = "3"; -- How much will the warning be visible
	})
	
	game.StarterGui:SetCore("SendNotification", {
		Title = "KKR HUB";  -- The title
		Text = "finish"; -- The text
		Icon = "rbxassetid://13867768098";
		Duration = "3"; -- How much will the warning be visible
	})
	script.Parent.Visible = false
	script.Parent.Parent.Frame2.Visible = true
	
end
coroutine.wrap(KSSKJ_fake_script)()
local function EIJWIMP_fake_script() -- Frame2.LocalScript2 
	local script = Instance.new('LocalScript', Frame2)

	script.Parent.PH.MouseButton1Click:Connect(function()
		script.Parent.Parent.Frame2.Visible = false
		script.Parent.Parent.FramePH.Visible = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "Phone"; -- The text
			Icon = "rbxassetid://13474640049";
			Duration = "3"; -- How much will the warning be visible
		})
	end)
	
	script.Parent.PC.MouseButton1Click:Connect(function()
		script.Parent.Parent.Frame2.Visible = false
		script.Parent.Parent.FramePC.Visible = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "KKR HUB";  -- The title
			Text = "PC"; -- The text
			Icon = "rbxassetid://13474626985";
			Duration = "3"; -- How much will the warning be visible
		})
	end)
end
coroutine.wrap(EIJWIMP_fake_script)()
